{
  "name": "retopractico3_lizvidal",
  "version": "1.0.0",
  "scripts": {
    "test:e2e": "cucumber-js --require-module ts-node/register tests/e2e/features",
    "test:api": "cucumber-js --require-module ts-node/register tests/api/features",
    "test:all": "npm run test:e2e && npm run test:api",
