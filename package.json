    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@cucumber/cucumber": "^9.5.1",
    "@playwright/test": "^1.40.1",
    "@types/node": "^20.10.6",
    "playwright": "^1.40.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}